   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('./data/musicfeatures.csv')\n",
    "\n",
    "upper = df['tempo'].quantile(.75)\n",
    "lower = df['tempo'].quantile(.25)\n",
    "\n",
    "df.loc[df['tempo'] > upper, 'tempo'] = 0\n",
    "df.loc[df['tempo'] < lower, 'tempo'] = 0\n",
    "\n",
    "print(df['tempo'].mean() + df['tempo'].std())"
   ]
  }
