    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('./data/airquality.csv')\n",
    "\n",
    "nrow = int(len(df) * 0.9)\n",
    "df90 = df.iloc[:nrow, ]\n",
    "mean = df90['Ozone'].mean()\n",
    "median1 = df90['Ozone'].median()\n",
    "df90 = df90.fillna({'Ozone': mean})\n",
    "median2 = df90['Ozone'].median()\n",
    "\n",
    "print(abs(median1 - median2))"
   ]
  }
