   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('./data/cats.csv')\n",
    "\n",
    "mean = df['Hwt'].mean()\n",
    "std = df['Hwt'].std()\n",
    "\n",
    "upper = mean + std * 1.5\n",
    "lower = mean - std * 1.5\n",
    "\n",
    "hwts = df.loc[(df['Hwt'] > upper) | (df['Hwt'] < lower)]['Hwt']\n",
    "print(hwts.mean())"
   ]
  }
